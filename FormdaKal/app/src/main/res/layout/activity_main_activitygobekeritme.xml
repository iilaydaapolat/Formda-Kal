<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4F1ED"
    tools:context=".MainActivitygobekeritme">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar6"
                android:layout_width="413dp"
                android:layout_height="53dp"
                android:background="#F4F1ED"
                android:minHeight="?attr/actionBarSize"
                android:theme="?attr/actionBarTheme"
                app:logo="@drawable/ip"
                app:navigationIcon="@drawable/back" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:background="#E8B0B0"
                android:text="GÖBEK ERİTEN KÜR TARİFLERİ"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="normal|bold" />

            <ImageView
                android:layout_marginStart="30dp"
                android:id="@+id/imageView9"
                android:layout_width="343dp"
                android:layout_height="297dp"
                app:srcCompat="@drawable/erit" />

            <com.google.android.gms.ads.AdView
                xmlns:ads="http://schemas.android.com/apk/res-auto"
                android:id="@+id/adView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_alignParentBottom="true"
                ads:adSize="SMART_BANNER"
                ads:adUnitId="ca-app-pub-1252159248405155/7295759254">
            </com.google.android.gms.ads.AdView>

            <TextView
                android:id="@+id/textView35"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="20dp"
                android:text="1-LİMONLU YOĞURT KÜRÜ"
                android:textColor="#000000"
                android:textSize="18sp"
                android:textStyle="normal|bold" />

            <TextView
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:id="@+id/textView36"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_marginTop="5dp"

                android:text="TextView"
                android:textColor="#000"
                android:textSize="16dp" />

            <TextView
                android:layout_marginTop="10dp"
                android:id="@+id/textView37"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginLeft="15dp"
                android:text="2-GÖBEK ERİTEN YOĞURTLU KÜR"
                android:textColor="#000000"
                android:textSize="18sp"
                android:textStyle="normal|bold" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="match_parent"
                android:layout_height="182dp"
                android:layout_marginTop="5dp"
                app:srcCompat="@drawable/lk" />

            <TextView
                android:layout_marginTop="3dp"
                android:id="@+id/textView38"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="TextView"
                android:textColor="#000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/textView39"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="1dp"
                android:layout_marginEnd="20dp"
                android:text="3-GÖBEK ERİTEN SOĞANLI KÜR"
                android:textColor="#000000"
                android:textColorHint="#000000"
                android:textSize="18sp"
                android:textStyle="normal|bold" />

            <TextView
                android:id="@+id/textView40"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="20dp"
                android:text="TextView"
                android:textColor="#000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/textView41"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:text="4-ALTIN YOĞURT KÜRÜ"
                android:textColor="#000000"
                android:textSize="18sp"
                android:textStyle="normal|bold" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="match_parent"
                android:layout_height="154dp"
                android:layout_marginTop="5dp"
                app:srcCompat="@drawable/ikii" />

            <TextView
                android:id="@+id/textView42"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="20dp"
                android:text="TextView"
                android:textColor="#000"
                android:textSize="16dp" />

            <TextView
                android:id="@+id/textView43"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="5dp"
                android:text="5-GÖBEK ERİTEN SODALI YOĞURT KÜRÜ"
                android:textColor="#000000"
                android:textSize="18sp"
                android:textStyle="normal|bold" />

            <TextView
                android:id="@+id/textView44"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="20dp"
                android:text="TextView"
                android:textColor="#000"
                android:textSize="16dp" />

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>